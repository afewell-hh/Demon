# Demon Environment Variables
# Copy this to .env and fill in your values
# DO NOT commit .env to version control

# NATS Configuration
NATS_URL=nats://127.0.0.1:4222
NATS_MON_PORT=8222
NATS_PORT=4222
RITUAL_STREAM_NAME=RITUAL_EVENTS

# Operate UI Feature Flags
# Comma-separated list: contracts-browser,canvas-ui,agent-flows
OPERATE_UI_FLAGS=

# JWT Authentication (for Agent Flow API)
# Required when agent-flows feature is enabled
# DO NOT COMMIT PRODUCTION SECRETS
JWT_SECRET=
JWT_ISSUER=
JWT_AUDIENCE=

# Schema Registry (for contracts browser feature)
SCHEMA_REGISTRY_URL=http://localhost:8080

# Admin Token (for admin endpoints)
ADMIN_TOKEN=
